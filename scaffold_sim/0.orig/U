FoamFile
{
    format      binary;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{

    defaultFaces
    {
    	type		noSlip;
    }
    
    "(walls|mesh_geometry)"
    {
    	type		noSlip;
    }
    
    inlet
    {
        type         		   fixedValue;
        volumetricFlowRate	   0.124; //15 mL/min
    }
        
