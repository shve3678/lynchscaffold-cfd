FoamFile
{
    format      binary;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{

    defaultFaces
    {
    	type		zeroGradient;
    }
    
    "(walls|mesh_geometry)"
    {
    	type		zeroGradient;
    }
    
    inlet
    {
        type           zeroGradient;
    }
    
    outlet
    {
        type            fixedValue;
        value           uniform 0;
    }

